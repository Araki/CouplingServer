<% content_for :title do %>画像管理<% end %>
<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>メンバーID</th>
      <th>メイン画像</th>
      <th>作成日時</th>
    </tr>
  </thead>
  <tbody>
<% @images.each do |image| %>
    <tr>
      <td><%= image.id %></td>
      <td><%= image.member_id %></td>
      <td><%= image.is_main %></td>
      <td><%= image.created_at %></td>
      <td>
        <%= link_to "詳細", admin_image_path(image) %>
      </td>
    </tr>
<% end %>
  </tbody>
</table>
<%= paginate @images %>